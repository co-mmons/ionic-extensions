    "intl:html-editor": "js-intl-google-sheet-importer --output-path src/html-editor/i18n --output-type json --document 1lTpcwU0ubFX7bJFCt8zeHYrwbDaoxNcjzpjUVdmiY_w --worksheet 1 --filterTags ionic-extensions-html-editor",
